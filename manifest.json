{
    "manifest_version": 2,

    "name": "Paywall Smasher",
    "description": "This extensions defeats paywalls on many news websites",
    "version": "1.0",

    "permissions": [
        "http://news.nationalpost.com/*",
        "http://www.thestar.com/*",
        "http://www.theonion.com/*"
    ],

    "content_scripts": [
        {
            "matches": [
                "http://*.nationalpost.com/*",
                "http://www.thestar.com/*",
                "http://www.theonion.com/*"
            ],
            "js": ["jquery-2.0.3.min.js", "del.js"],
            "all_frames": true
        }
    ]
}

